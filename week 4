
 /*query 4*/
  select * from branch;
select branch_name
from branch
where asets>(select  max(asets) from branch where branch_city="bangalore");

/*query 5*/
 delete
 from bank_account
 where branch_name in(select branch_name  from branch where branch_city="bombay");
 
